{
  "timezone": "Europe/Copenhagen",
  "extends": [
    ":automergeBranch",
    ":automergePatch",
    ":automergeStableNonMajor",
    ":disableDependencyDashboard",
    ":disableRateLimiting",
    ":ignoreModulesAndTests",
    ":ignoreUnstable",
    ":prImmediately",
    ":renovatePrefix",
    ":semanticCommits",
    ":semanticPrefixChore",
    ":separateMajorReleases",
    ":skipStatusChecks",
    ":updateNotScheduled",
    "group:allNonMajor",
    "group:monorepos",
    "group:recommended",
    "helpers:disableTypesNodeMajor",
    "workarounds:all"
  ],
  "rangeStrategy": "bump"
}
